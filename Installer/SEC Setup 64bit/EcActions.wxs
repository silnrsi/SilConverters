            <Custom Action="Set_WORD_DOT_FOLDER_via_folder" After="Set_WORD_DOT_FOLDER_via_startup_folder"><![CDATA[WORD_DOT_FOLDER_FOLDER AND NOT WORD_DOT_STARTUP_FOLDER AND NOT WORD_DOT_FOLDER]]></Custom>
